class Solution {
  public:
    int missingNum(vector<int>& arr)
    {
      int n=arr.size()+1;
      int sum_of_n_num=0;
      //handling overflow 
      if(n%2==0)               
      sum_of_n_num=n/2*(n+1);  
      else                     
      sum_of_n_num=(n+1)/2*n;
      int sum_of_array=0;
      // array sum
      for(int i=0;i<n-1;i++)
      {
          sum_of_array+=arr[i];
      }
      return (sum_of_n_num-sum_of_array);
      
    }
};
